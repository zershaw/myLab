<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.cbsys.saleexplore.idao.IStoreBrandDAO">

    <select id="getStoreBrandIds" resultType="Integer">
        SELECT brandId
        FROM T_STORE_BRAND
        <where>
            storeId = #{storeId}
        </where>
    </select>



</mapper>